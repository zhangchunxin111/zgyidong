//PC端流量处理 ----查看是否能判断和提交
function fun_flowpinfo2(needShowWait){
	var pho = document.getElementById("flow_pho").value;
	
	if(/^\d+$/.test(pho)){
		if(pho.length>=3){
			var sph = "";
			var checkb = false;
			if(pho.length>=7){
				sph = pho.substring(0,7);
				checkb = (isPhoneNumber(sph+"1234")==0);
			} else if(pho.length==11){
				sph = pho;
				checkb = (isPhoneNumber(sph)==0);
			} else {
				sph = pho.substring(0,3);
				checkb = (isPhoneNumber(sph+"12345678")==0);
			}
			if(checkb){
				fun_hiddenflowerr();
				if((pho.length==7 || pho.length==11) && flow_pinfo_last != pho.substring(0,7)){
					flow_pinfo_status = 0;
				}
				if((pho.length==7 || pho.length==11) && flow_pinfo_status == 0){
					//执行
					flow_pinfo_last=pho.substring(0,7);
					var wrong = function(){
						fun_showflowerr("已断开,请重试");
					}
					var tid = null;
					if(needShowWait){
						fun_showflowerr("请稍候...");
						tid = window.setTimeout(wrong, 5000);
					}
					//Ajax请求服务器，获得结果（是否正确获得信息,省URL或错误代码）
					var resfun= function(rst){
						var rs = rst.split(" ");
						if(rs[0]=="Y"){
							var bif = getProvBrief(rs[1]);
							var nomsg = "归属省未开通该项业务";
							nomsg = "非本省号码";

							var flowurl = "http://shop.10086.cn/i/?f=rechargegprs";
							if(flowurl==null || flowurl==""){//正常不会被执行
								clearTO(tid);
								flow_pinfo_status=0;
								fun_showflowerr(nomsg);
							} else {

								if(location.pathname.toLowerCase().indexOf(bif)!=-1){
									cmcurprov = getProvCity();
									flow_pinfo_status=1;//已获得
									clearTO(tid);
									fun_hiddenflowerr();

								}
								else{
									clearTO(tid);
									flow_pinfo_status=0;
									fun_showflowerr(nomsg);
								}
							}
							
						} else if (rs[0]=="N"){
							clearTO(tid);
							flow_pinfo_status=0;
							fun_showflowerr("请输入正确的移动号码");
						}
					}

					//执行Ajax
					var majax = false;
					if (window.XMLHttpRequest) {
						majax = new XMLHttpRequest();
					} else if (window.ActiveXObject) {
						majax = new ActiveXObject("Microsoft.XMLHTTP");
					}
					if (majax) {
						try{
							majax.open("GET", "/service/prov/prov.jsp?phone=" + pho.substring(0,7) + "1234",false);
							majax.onreadystatechange = function() {
								if (majax.readyState == 4) {
									if(majax.status == 200){
										resfun(majax.responseText);
									}else{
										clearTO(tid);
										flow_pinfo_status=0;
										if(needShowWait){
											fun_showflowerr("已断开,请重试");
										}
									}
								}
							}
							//majax.send(null);
						}catch(te){
							flow_pinfo_status=0;
							if(needShowWait){
								fun_showflowerr("已断开,请重试");
							}
						}
					} else {
						clearTO(tid);
						if(needShowWait){
							alert("您的浏览器不支持Ajax实现!");
						}
					}
				}
			} else {
				fun_showflowerr("请输入正确的移动号码");
			}
		} else {
			fun_hiddenflowerr();
		}
	} else if (pho.length>0){
		fun_showflowerr("请输入正确的移动号码");
	} else {
		fun_hiddenflowerr();
	}
}

//PC端流量处理 ----查看是否能判断和提交
function m_fun_flowpinfo2(needShowWait){
	var pho = document.getElementById("m_flow_pho").value;
	
	if(/^\d+$/.test(pho)){
		if(pho.length>=3){
			var sph = "";
			var checkb = false;
			if(pho.length>=7){
				sph = pho.substring(0,7);
				checkb = (isPhoneNumber(sph+"1234")==0);
			} else if(pho.length==11){
				sph = pho;
				checkb = (isPhoneNumber(sph)==0);
			} else {
				sph = pho.substring(0,3);
				checkb = (isPhoneNumber(sph+"12345678")==0);
			}
			if(checkb){
				m_fun_hiddenflowerr();
				if((pho.length==7 || pho.length==11) && m_flow_pinfo_last != pho.substring(0,7)){
					m_flow_pinfo_status = 0;
				}
				if((pho.length==7 || pho.length==11) && m_flow_pinfo_status == 0){
					//执行
					m_flow_pinfo_last=pho.substring(0,7);
					var wrong = function(){
						m_fun_showflowerr("已断开,请重试");
					}
					var tid = null;
					if(needShowWait){
						m_fun_showflowerr("请稍候...");
						tid = window.setTimeout(wrong, 5000);
					}
					//Ajax请求服务器，获得结果（是否正确获得信息,省URL或错误代码）
					var resfun= function(rst){
						var rs = rst.split(" ");
						if(rs[0]=="Y"){
							var bif = getProvBrief(rs[1]);
							var nomsg = "归属省未开通该项业务";
							nomsg = "非本省号码";

							var flowurl = "http://shop.10086.cn/i/?f=rechargegprs";
							if(flowurl==null || flowurl==""){//正常不会被执行
								clearTO(tid);
								m_flow_pinfo_status=0;
								m_fun_showflowerr(nomsg);
							} else {

								if(location.pathname.toLowerCase().indexOf(bif)!=-1){
									cmcurprov = getProvCity();
									m_flow_pinfo_status=1;//已获得
									clearTO(tid);
									m_fun_hiddenflowerr();

								}
								else{
									clearTO(tid);
									m_flow_pinfo_status=0;
									m_fun_showflowerr(nomsg);
								}
							}
							
						} else if (rs[0]=="N"){
							clearTO(tid);
							m_flow_pinfo_status=0;
							m_fun_showflowerr("请输入正确的移动号码");
						}
					}

					//执行Ajax
					var majax = false;
					if (window.XMLHttpRequest) {
						majax = new XMLHttpRequest();
					} else if (window.ActiveXObject) {
						majax = new ActiveXObject("Microsoft.XMLHTTP");
					}
					if (majax) {
						try{
							majax.open("GET", "/service/prov/prov.jsp?phone=" + pho.substring(0,7) + "1234",false);
							majax.onreadystatechange = function() {
								if (majax.readyState == 4) {
									if(majax.status == 200){
										resfun(majax.responseText);
									}else{
										clearTO(tid);
										m_flow_pinfo_status=0;
										if(needShowWait){
											m_fun_showflowerr("已断开,请重试");
										}
									}
								}
							}
							//majax.send(null);
						}catch(te){
							m_flow_pinfo_status=0;
							if(needShowWait){
								m_fun_showflowerr("已断开,请重试");
							}
						}
					} else {
						clearTO(tid);
						if(needShowWait){
							alert("您的浏览器不支持Ajax实现!");
						}
					}
				}
			} else {
				m_fun_showflowerr("请输入正确的移动号码");
			}
		} else {
			m_fun_hiddenflowerr();
		}
	} else if (pho.length>0){
		m_fun_showflowerr("请输入正确的移动号码");
	} else {
		m_fun_hiddenflowerr();
	}
}

//PC端提交
function fun_czsubmit2(){
	fun_chama("INDEX_HOME_C_KJ_CZ_CHF_"+CmLocation);
	var pho = document.getElementById("cz_pho").value;
	
	if (pho == "请输入手机号码") {
		$("#cz_pho").css("color","#ff0000");
		$("#cz_pho").css("font-family","微软雅黑");
		return;
	} 
	
	if(isPhoneNumber(pho) != 0){
		fun_showerr("请输入正确的移动号码");
		//fun_chama("INDEX_LJCZ_SB");
		return;
	}


	var sum = document.getElementById("cz_val").value;
	if(sum!=30 && sum != 50 && sum!=100 && sum!=300 && sum!=500 && sum!=0){//正常不会被执行
		fun_showerr("请重新选择金额");
		//fun_chama("INDEX_LJCZ_SB");
		return;
	}
	fun_pinfo2(true);
	document.getElementById("cz_form").submit();
	/*if (!fun_iserrblock()) {
		if (cz_pinfo_status==1) {
			cz_pinfo_status = 0;
			//fun_chama("INDEX_LJCZ_" + cmcurprov);
			//fun_chama2();
			document.getElementById("cz_form").submit();
		}
		else if(cz_pinfo_status==0) {
			fun_pinfo2(true);
			if(cz_pinfo_status == 1){
				cz_pinfo_status = 0;
				//fun_chama("INDEX_LJCZ_" + cmcurprov);
				//fun_chama2();
				document.getElementById("cz_form").submit();
			}
			else {
				//fun_chama("INDEX_LJCZ_SB");
			}
		}
	}else{
		//fun_chama("INDEX_LJCZ_SB");
	}*/
}

