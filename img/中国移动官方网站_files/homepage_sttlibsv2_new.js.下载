	$(v).find(".listbox a").each(function(index, obj) {
    	$(obj).attr("wtCode","INDEX_HOME_C_FZXQ_" + (e + 1 ) + "_" + (index + 1) + "_" + CmLocation);
    	$(obj).on("click", wtFunction);
    	var hostimg = $(obj).attr("hotimg");
		if (hostimg != "" && hostimg != "null") {
         	var HotImg = $('<img>');
            HotImg.attr("class", "serviceHotImg");
            HotImg.attr("src", hostimg);
            $(obj).append(HotImg);
        }
    });
	
});

