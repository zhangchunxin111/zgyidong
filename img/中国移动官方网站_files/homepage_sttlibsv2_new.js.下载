			//判断IPAD端处理显示推荐词,多余的隐藏
			function is_iPad(){ 
				var ua = navigator.userAgent.toLowerCase(); 
				if(ua.match(/iPad/i)=="ipad") { 
				   return true; 
				} else { 
				   return false; 
				} 
			}
			if(is_iPad()){
			   $('.allService').attr('href','javascript:;');
			   $('.louti').each(function(x){
			     if($(this).find('ul.list li').length > 4){
				   $(this).find('ul.list li:gt(3)').css('display','none');
				 }
			   });
			   
			   //全部服务下拉
				$('.allService').click(function(){
				   if($('#nav235').css('display') == 'none'){
						if($('.list235_pad').css('display') == 'none'){
							$('.list235_pad').css('display','block');
						}
						else if($('.list235_pad').css('display') == 'block'){
						   $('.list235_pad').css('display','none');
						}
					}
				});
			   
			}
			
			
			
