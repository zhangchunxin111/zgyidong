// 商城销售平台接口调用
if005GetPropertyOption();

//商品价格同步
$(function(){
	//bSyncPrice();
	
	var hostname = "www1.10086.cn";
	var arrSpanObj = [];
	var goodsSkuId = '';
	$("span[data-goodsid]").each(function(index, obj) {
		var goodsid = $(obj).attr("data-goodsid");
		var skuid = $(obj).attr("data-skuid");
		if(goodsid && skuid) {
			goodsSkuId += encodeURIComponent("goodsId:" + goodsid + ",skuId:" + skuid) + "_";
			arrSpanObj.push(obj);
		}
	});
	if (arrSpanObj.length > 0) {
		drawPrice(goodsSkuId, arrSpanObj);
	}
});

function drawPrice(goodsSkuId, arrSpanObj) {
	var hostname = "www1.10086.cn";
	$.ajax({
		//async: false,
		async: true,
		url: "http://" + hostname + "/service/homepageprice/homepageprice.jsp?mcmsPrice=" + goodsSkuId,
		dataType: "jsonp",
		jsonp: "callback", 
        jsonpCallback: "jsoncallback",
		success: function(jsonData) {	
			for (i = 0; i < arrSpanObj.length; i++) {
				if (jsonData && jsonData.priceData[i] !== "undefined" && jsonData.priceData[i]) {
					var tmpprice = (jsonData.priceData[i].cmPriceInterface).substring(1);
					if (tmpprice !== "售罄") {
						tmpprice += "元";
					}
					$(arrSpanObj[i]).html(tmpprice);	
				}
			}
		}
	});
}

