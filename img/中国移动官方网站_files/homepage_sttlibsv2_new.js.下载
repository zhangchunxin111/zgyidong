//插码函数，避免异常
function fun_chama(sign){
	try{
		if (typeof(_tag)!= 'undefined'){
			_tag.dcsMultiTrack('WT.event', sign);
		}
	}catch(t){}
}

//分号码、金额插码
function fun_chama2(){
	try{
		var amountobj = document.getElementById("cz_val");
		var mobileNoobj = document.getElementById("cz_pho");
		if (typeof(_tag)!= 'undefined'){
			_tag.dcsMultiTrack('WT.INDEX_mobile',mobileNoobj.value,'WT.INDEX_money',amountobj.value);
		}
	}catch(t){}
}

//----------------充值手机号判断--------------------

//初始化手机号码框

function fun_showerr(str){
	setInnerText(document.getElementById("cz_div_notice"),str);
	fun_showdiv("cz_div_notice");
}

function m_fun_showerr(str){
	setInnerText(document.getElementById("m_cz_div_notice"),str);
	fun_showdiv("m_cz_div_notice");
}
function fun_hiddenerr(){
	fun_hiddendiv("cz_div_notice");
}

function m_fun_hiddenerr(){
	fun_hiddendiv("m_cz_div_notice");
}
function fun_iserrblock(){
	return (document.getElementById("cz_div_notice").style.display=="block");
}
function m_fun_iserrblock(){
	return (document.getElementById("m_cz_div_notice").style.display=="block");
}

//初始化流量手机号码框
function fun_initflowpho(){
	var pho = getCookie("flowonehis");
	if(pho!=null && isPhoneNumber(pho) == 0){
		document.getElementById("flow_pho").value=pho;
	}
}

function m_fun_initflowpho(){
	var pho = getCookie("m_flowonehis");
	if(pho!=null && isPhoneNumber(pho) == 0){
		document.getElementById("m_flow_pho").value=pho;
	}
}
function fun_showflowerr(str){
	setInnerText(document.getElementById("flow_div_notice"),str);
	fun_showdiv("flow_div_notice");
}

function m_fun_showflowerr(str){
	setInnerText(document.getElementById("m_flow_div_notice"),str);
	fun_showdiv("m_flow_div_notice");
}
function fun_hiddenflowerr(){
	fun_hiddendiv("flow_div_notice");
}

function m_fun_hiddenflowerr(){
	fun_hiddendiv("m_flow_div_notice");
}
function fun_iserrflowblock(){
	return (document.getElementById("flow_div_notice").style.display=="block");
}
function m_fun_iserrflowblock(){
	return (document.getElementById("m_flow_div_notice").style.display=="block");
}

//------------------------------------------------------------------------------

function fun_tocamountdis(){
	var o = document.getElementById("cz_amall");
	if(o.style.display=="block"){
		o.style.display="none"
	} else {
		o.style.display="block"
	}
}

//清除定时任务
function clearTO(tid){
	try{
		window.clearTimeout(tid);
	}catch(e){}
}

function setInnerText(obj,str){
	while (obj.childNodes.length != 0) {
		obj.removeChild(obj.childNodes[0]);
	}
	var objp = document.createElement("p"); 
	obj.appendChild(objp);
	objp.appendChild(document.createTextNode(str));
}

function fun_showfs(){
	fun_showdiv("cz_fs");
}

function fun_hidfs(){
	fun_hiddendiv("cz_fs");
}

function fun_showdiv(oid){
	var o = document.getElementById(oid);
	if(o){
		o.style.display = "block";
	}
}

function fun_hiddendiv(oid){
	var o = document.getElementById(oid);
	if(o){
		o.style.display = "none";
	}
}
