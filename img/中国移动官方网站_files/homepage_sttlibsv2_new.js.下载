							clearInterval( timer2 );
							for(var i=0;i<oLis.length;i++){
								oLis[i].className = '';
								oNavbox[i].style.display = 'none';		
							}
							this.className = 'cur';
							oNavbox[this.index].style.display = 'block';	
						}
						oLis[i].onmouseout = function(){
							num = this.index;
							timer2 = setTimeout(function(){
								oLis[num].className = '';
								oNavbox[num].style.display = 'none';
							},400);	
						}
					}
				}
					
				if (oNavbox !== 'undefined' && oNavbox) {	
					for(var i=0;i<oNavbox.length;i++){
						oNavbox[i].index = i;
						oNavbox[i].onmouseover = function(){
							clearInterval( timer2 );
							for(var i=0;i<oLis.length;i++){
								oLis[i].className = '';
								oNavbox[i].style.display = 'none';		
							}
							oLis[this.index].className = 'cur';
							this.style.display = 'block';	
						}
						oNavbox[i].onmouseout = function(){
							num = this.index;
							timer2 = setTimeout(function(){
								oLis[num].className = '';
								oNavbox[num].style.display = 'none';
							},400);	
						}
					}
				}
			}
			
