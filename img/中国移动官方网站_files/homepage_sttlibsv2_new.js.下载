//反转义
function decodeHtml(s){
	var d = document.createElement('div');
	d.innerHTML = s;
	return $(d).text();
}

//是否手机访问
function isPhone(){
	if (navigator.userAgent.match(/(phone|pad|pod|iPhone|iPod|ios|iPad|Android|Mobile|BlackBerry|IEMobile|MQQBrowser|JUC|Fennec|wOSBrowser|BrowserNG|WebOS|Symbian|Windows Phone)/i)) {
		return true;
	}
	return false;
}

//公告区 20170615添加

function loadNotice(){
	
	// 集团新闻 第一条
	$.get("/aboutus/news/groupnews/1_general.js",function(rep){

        var obj = JSON.parse(rep.substring(rep.indexOf("=")+1,rep.indexOf("}]}")+3))

        var arrcData = obj['cData'];
		
		for (var i=0;i<arrcData.length;i++) {
			$(".indexgg ul li:eq(0) a").attr("href",arrcData[i].href).attr("title",decodeHtml(decodeHtml(arrcData[i].title1))).html(decodeHtml(decodeHtml(arrcData[i].title1)));
			$(".indexgg ul li:eq(0) em").html(decodeHtml(decodeHtml(arrcData[i].publish_time)).substring(0,10));
			break;
		}
	})
	// 省公司新闻公告 第一条
	$.get("/aboutus/news/pannounce/"+getCurProvBrief2()+"/1_general.js",function(rep){

        var obj = JSON.parse(rep.substring(rep.indexOf("=")+1,rep.indexOf("}]}")+3))

        var arrcData = obj['cData'];
		
		for (var i=0;i<arrcData.length;i++) {
			$(".indexgg ul li:eq(1) a").attr("href",arrcData[i].href).attr("title",decodeHtml(decodeHtml(arrcData[i].title1))).html(decodeHtml(decodeHtml(arrcData[i].title1)));
			$(".indexgg ul li:eq(1) em").html(decodeHtml(decodeHtml(arrcData[i].publish_time)).substring(0,10));
			break;
		}
	})
	// 集团公告 第一条
	$.get("/aboutus/news/announced/1_general.js",function(rep){

        var obj = JSON.parse(rep.substring(rep.indexOf("=")+1,rep.indexOf("}]}")+3))

        var arrcData = obj['cData'];
		arrcData = getRoamingNotice(arrcData);
		
		for (var i=0;i<arrcData.length;i++) {
			$(".indexgg ul li:eq(2) a").attr("href",arrcData[i].href).attr("title",decodeHtml(decodeHtml(arrcData[i].title1))).html(decodeHtml(decodeHtml(arrcData[i].title1)));
			$(".indexgg ul li:eq(2) em").html(decodeHtml(decodeHtml(arrcData[i].publish_time)).substring(0,10));
			break;
		}
	})
	// 省公司新闻公告 第二条
	$.get("/aboutus/news/pannounce/"+getCurProvBrief2()+"/1_general.js",function(rep){

        var obj = JSON.parse(rep.substring(rep.indexOf("=")+1,rep.indexOf("}]}")+3))

        var arrcData = obj['cData'];
		
		for (var i=1;i<arrcData.length;i++) {
			$(".indexgg ul li:eq(3) a").attr("href",arrcData[i].href).attr("title",decodeHtml(decodeHtml(arrcData[i].title1))).html(decodeHtml(decodeHtml(arrcData[i].title1)));
			$(".indexgg ul li:eq(3) em").html(decodeHtml(decodeHtml(arrcData[i].publish_time)).substring(0,10));
			break;
		}
	})
}


//获取国漫公告数据列表
function getRoamingNotice(data){
	
	$.ajaxSetup({    
        async : false    
    }); 
	
	var arrcData1 = "";
	var arrcData2 = "";
	$.get("/roaming/notice/1_general.js",function(rep){

        var obj = JSON.parse(rep.substring(rep.indexOf("=")+1,rep.indexOf("}]}")+3))

        arrcData1 = obj['cData'];
		
	})
	
	if(arrcData1 == ""){
		arrcdata2 = data;
	}else{
		arrcData2 = data.concat(arrcData1);
		arrcData2.sort(function(a,b){
			if(new Date(a.publish_time).getTime() < new Date(b.publish_time).getTime()){
				return 1;
			}else if(new Date(a.publish_time).getTime() > new Date(b.publish_time).getTime()){
				return -1;
			}else{
				return 0;
			}
		})
	}
	return arrcData2;
}

//国资动态 20170615添加
function loadGzdt(){
	$.get("/aboutus/news/sasac/1_general.js",function(rep){

        var obj = JSON.parse(rep.substring(rep.indexOf("=")+1,rep.indexOf("}]}")+3))

        var arrcData = obj['cData'];
		
		for (var i=0;i<arrcData.length;i++) {
			if (i==4) {
				break;
			}
			$(".indexgg2 li:eq("+i+") a").attr("href",arrcData[i].href).attr("title",decodeHtml(decodeHtml(arrcData[i].title1))).html(decodeHtml(decodeHtml(arrcData[i].title1)));
			$(".indexgg2 li:eq("+i+") span").html(decodeHtml(decodeHtml(arrcData[i].publish_time)).substring(0,10));
			$(".indexgg2 li:eq("+i+")").show();
		}
		if(isPhone()){
			$(".indexgg2 li:eq(2),.indexgg2 li:eq(3)").hide();
		}
	})
}
